{
  "name": "schema-visualization",
  "displayName": "Schema Visualization",
  "description": "Erd, Er-diagram. Visualization of databases",
  "version": "0.9.4",
  "publisher": "R0tenur",
  "license": "MIT",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/R0tenur/visualization.git"
  },
  "bugs": {
    "url": "https://github.com/R0tenur/visualization/issues"
  },
  "engines": {
    "vscode": "*",
    "azdata": "*"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "dashboard.tabs": [
      {
        "id": "visualization-panel",
        "title": "Schema Visualization",
        "description": "Schema visualization",
        "container": {
          "webview-container": "diagram-panel"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pack": "vsce package",
    "test": "jest --config=jest.config.js"
  },
  "devDependencies": {
    "@types/azdata": "1.33.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^22.13.14",
    "@types/vscode": "^1.92.0",
    "azdata-test": "^1.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.0",
    "typescript": "^5.5.4",
    "vsce": "^1.71.0",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "svg-to-pdfkit": "^0.1.8"
  }
